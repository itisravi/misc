colorscheme elflord
set number
set modeline
set expandtab
set cursorline
set ls=2
set incsearch
set formatoptions+=w
set tw=80
map <F2> : set number!<CR>
map <F3> : TlistToggle<CR>
map <F4> : set nowrap!<CR>
map <F5> : set hlsearch!<CR>
map <F6> : set cursorline!<CR>
map <F7> : set expandtab!<CR>
map <F8> : NERDTreeToggle<CR>
map <F9> : set relativenumber!<CR>
